{
  "name": "@fabrix/spool-vault",
  "version": "1.1.0-alpha.1",
  "description": "Spool: Vault for Fabrix to implement HashiCorp's https://vaultproject.io/",
  "homepage": "https://fabrix.app",
  "author": {
    "name": "scott-wyatt",
    "email": "scottwyatt86@gmail.com",
    "url": "https://cali-style.com"
  },
  "keywords": [
    "valut",
    "HashiCorp",
    "fabrix",
    "trailjs"
  ],
  "scripts": {
    "build": "tsc -p ./lib/tsconfig.release.json",
    "lint": "tslint -p ./lib",
    "watch": "tsc -w -p ./lib/tsconfig.release.json",
    "test": "npm run clean && npm run lint && npm run build && nyc mocha",
    "prepublishOnly": "npm run compile",
    "compile": "npm run clean && npm run build",
    "clean": "rm -rf dist",
    "ci": "cd .. && ci"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "pre-commit": [
    "test"
  ],
  "dependencies": {
    "joi": "^13.4.0",
    "node-vault": "^0.9.0"
  },
  "devDependencies": {
    "@fabrix/fabrix": "^1.1.3",
    "@fabrix/lint": "^1.0.0-alpha.3",
    "@types/node": "~10.3.4",
    "mocha": "^5",
    "nyc": "^12.0.2",
    "smokesignals": "^2.0.0",
    "sqlite3": "^4.0.0",
    "supertest": "^3.1.0",
    "tslib": "~1.9.0",
    "tslint": "~5.10.0",
    "tslint-microsoft-contrib": "~5.0.3",
    "tsutils": "~2.27.1",
    "typescript": "~2.8.1"
  },
  "peerDependencies": {
    "@fabrix/fabrix": "^1.1.4"
  },
  "engines": {
    "node": ">= 7.6.0 =< 10.0.0",
    "npm": ">= 3.10.0"
  },
  "repository": "git@github.com:fabrix-app/spool-vault.git",
  "license": "MIT"
}
